{
  "check-coverage": true,
  "per-file": true,
  "skip-full": true,
  "all": true,
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/tests/**",
    "src/config/**",
    "src/utils/importData.ts",
    "src/**/*.d.ts",
    "src/index.ts"
  ],
  "reporter": [
    "html",
    "lcov",
    "text",
    "text-summary"
  ],
  "report-dir": "./coverage",
  "temp-dir": "./.nyc_output",
  "branches": 70,
  "lines": 80,
  "functions": 80,
  "statements": 80
}
